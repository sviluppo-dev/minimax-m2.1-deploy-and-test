# MiniMax Deploy Configuration
# Copy to .env and fill in your values

# ===========================================
# Infisical Secrets Management (recommended)
# ===========================================
# Set these to auto-load WANDB_API_KEY, HF_TOKEN from Infisical
# INFISICAL_HOST=https://app.infisical.com
# INFISICAL_CLIENT_ID=your-machine-identity-client-id
# INFISICAL_CLIENT_SECRET=your-machine-identity-client-secret
# INFISICAL_PROJECT_ID=your-project-id
# INFISICAL_ENV=dev

# ===========================================
# Or set secrets directly (fallback)
# ===========================================
# WANDB_API_KEY=your-api-key
# HF_TOKEN=your-hf-token

# ===========================================
# vLLM Server
# ===========================================
MINIMAX_VLLM_BASE_URL=http://localhost:8000/v1
MINIMAX_MODEL_NAME=MiniMaxAI/MiniMax-M1-80k

# ===========================================
# Benchmark Settings
# ===========================================
MINIMAX_BENCHMARK_NUM_REQUESTS=100
MINIMAX_BENCHMARK_CONCURRENCY=10
MINIMAX_BENCHMARK_MAX_TOKENS=256
MINIMAX_BENCHMARK_WARMUP_REQUESTS=5

# ===========================================
# Weights & Biases
# ===========================================
MINIMAX_WANDB_PROJECT=minimax-m2.1-baseline
# MINIMAX_WANDB_ENTITY=your-team

# ===========================================
# API Server
# ===========================================
MINIMAX_API_HOST=0.0.0.0
MINIMAX_API_PORT=8080
